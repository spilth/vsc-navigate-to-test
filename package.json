{
    "name": "navigate-to-test",
    "displayName": "Naviate to Test",
    "description": "Easily navigate between implementation code and test code with a shortcut.",
    "version": "0.0.2",
    "publisher": "spilth",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.navigateToTest"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.navigateToTest",
                "title": "Navigate to Test"
            }
        ],
        "keybindings": [
			{
				"command": "extension.navigateToTest",
				"key": "Alt+T"
			}
		]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
